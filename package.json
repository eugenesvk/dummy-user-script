{
  "name": "user_script_wasm",
  "version": "0.1.0",
  "author": "es",
  "packageManager": "yarn@3.6.1",
  "private": true,
  "scripts": {
    "b": "run-s rust2wasm wasm2js_ wasm2js2bundle",
    "b_old": "rimraf -g 'dist/js/*' && rollup --bundleConfigAsCjs --config rollup-wasm-rust.js",
    "w": "rimraf -g 'dist/js/*' && rollup --bundleConfigAsCjs --config rollup-wasm-rust.js --watch",
    "t": "rimraf -g 'dist/js/*' && rollup --bundleConfigAsCjs --config rollup-wasm1.js",
    "rust2wasm": "wasm-pack build --target bundler --out-dir 'pkg' --no-typescript --no-pack",
    "rust2wasm-dev": "wasm-pack build --target bundler --out-dir 'pkg' --no-typescript --no-pack --dev",
    "wasm2js_": "wasm2js pkg/user_script_wasm_bg.wasm -o pkg/user_script_wasm_bg.wasm.js",
    "wasm2js2bundle": "rimraf -g 'dist/js/*' && rollup --bundleConfigAsCjs --config rollup-jswasm.js"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-wasm": "^6.1.3",
    "@wasm-tool/rollup-plugin-rust": "^2.4.0",
    "binaryen": "^114.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-userscript": "^0.1.1"
  }
}
